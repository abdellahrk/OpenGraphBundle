{
    "name": "abdellahramadan/open-graph-bundle",
    "description": "A bundle to add open-graph to a website",
    "type": "symfony-bundle",
    "require": {
        "twig/twig": "^3.0",
        "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
        "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0",
        "symfony/cache": "^7.2",
        "symfony/config": "^7.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Abdellahramadan\\OpenGraphBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Abdellahramadan\\OpenGraphBundle\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Abdellah Ramadan",
            "email": "ramadanabdel24@gmail.com"
        }
    ],
    "require-dev": {
        "symfony/test-pack": "^1.0",
        "symfony/phpunit-bridge": "^7.2",
        "phpstan/phpstan": "^2.1"
    }
}
